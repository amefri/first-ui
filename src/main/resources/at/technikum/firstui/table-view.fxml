<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="at.technikum.firstui.view.TableView"
      prefHeight="400.0" prefWidth="600.0">

    <VBox HBox.hgrow="NEVER">

                <TableView VBox.vgrow="ALWAYS" fx:id="tableView">
                    <columns>
                        <TableColumn text="C1" fx:id="col1"/>
                        <TableColumn text="C2" fx:id="col2" />
                        <TableColumn text="C3" fx:id="col3"/>
                        <TableColumn text="C4" fx:id="col4"/>
                        <TableColumn text="C5" fx:id="col5"/>
                    </columns>
                </TableView>



    </VBox>

</VBox>