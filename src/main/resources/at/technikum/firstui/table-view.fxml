<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.collections.FXCollections?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="at.technikum.firstui.view.TableView"
      prefHeight="400.0" prefWidth="600.0">

    <VBox HBox.hgrow="NEVER">
                <TableView VBox.vgrow="ALWAYS" fx:id="tableView">
                    <columns>
                        <TableColumn text="Tour" fx:id="col1"/>
                        <TableColumn text="Date" fx:id="col2" />
                        <TableColumn text="Duration" fx:id="col3"/>
                        <TableColumn text="Distance" fx:id="col4"/>
                    </columns>
                </TableView>
    </VBox>

</VBox>